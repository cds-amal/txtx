module "manual" {
  name = "Manual 101"
  description = "Lorem ipsum dolor sit amet"
}

variable "say_hello" {
  description = "Greeting variable"
  value = "Hello"
}

variable "to" {
  description = "Destination"
  value = "World"
}

output "say_hello_world" {
  description = "Combining variables"
  value = "${variable.say_hello}, ${variable.to}! ${stacks_encode_ok(1)}"
}

output "output_no_deps" {
  description = "Destination"
  value = "ok"
}
