{
    "name": "Pyth",
    "default_workspace": "development",
    "environnements": {
      "development": {
        "stacks_network": "testnet",
        "stacks_rpc_url": "http://localhost:20443",
        "pyth_rpc_url": "https://hermes.pyth.network"
      },
      "staging": {
        "stacks_network": "testnet",
        "stacks_rpc_url": "https://api.staging.hiro.so:20443"
      },
      "production": {
        "stacks_network": "mainnet",
        "stacks_rpc_url": "https://api.staging.hiro.so:20443"
      }
    },
    "runbooks": [
      {
        "name": "Read Price Feed",
        "description": "This runbook reads the price for a given Price feed ID.",
        "location": "./run/price-feeds/read.tx"
      },
      {
        "name": "Update Price Feed",
        "description": "This runbook updated the price of a feed.",
        "location": "./run/price-feeds/update.tx"
      }
    ]
}
